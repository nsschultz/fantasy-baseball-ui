{ "port": 5545, "protocol": "http", "name": "players", "defaultResponse": { "statusCode": 400, "body": "Bad Request", "headers": {} },
  "stubs": [
    {
      "predicates": [{ "equals": { "method": "GET", "path": "/api/health" } }],
      "responses": [{ "is": { "statusCode": 200, "body": "Healthy" } }]
    },
    {
      "predicates": [{ "equals": { "method": "GET", "path": "/api/v2/action/export" } }],
      "responses": [{ 
        "is": { 
          "statusCode": 200, 
          "body": "<%- stringify(filename, './responses/players.csv') %>",
