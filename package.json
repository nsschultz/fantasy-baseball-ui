{
  "name": "fantasy-baseball-ui",
  "version": "1.2.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@mui/icons-material": "^5.10.16",
    "@mui/lab": "^5.0.0-alpha.110",
    "@mui/material": "^5.10.16",
    "@mui/styles": "^5.10.16",
    "axios": "^1.2.0",
    "eslint-plugin-jest": "^27.1.6",
    "file-saver": "^2.0.5",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint src",
    "test": "CI=true react-scripts test --silent --coverage --testResultsProcessor jest-sonar-reporter --detectOpenHandles",
    "sonar": "node sonar-project.js",
    "build:default": "react-scripts build",
    "build:docker": "npm run build:default && npm run lint && npm run test && npm run sonar"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.5.4",
    "@babel/preset-react": "^7.18.6",
    "babel-jest": "^29.3.1",
    "eslint-plugin-sonarjs": "^0.16.0",
    "react-test-renderer": "^17.0.2",
    "sonarqube-scanner": "^2.8.2"
  },
  "jest": {
    "clearMocks": true
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
